public class fibonacci{
    public static void main(String[] args) {

        int count = 15, a = 0, b = 1;
        System.out.print("Fibonacci Series of "+count+" numbers:");

        for (int i = 1; i <= count; ++i)
        {
            System.out.print(a+" ");
            int c = a + b;
            a = b;
            b = c;
        }
    }
}
